{
  "file": "/data/config/paper-global.yml",
  "ops": [
    {
      "$set": {
        "path": "$.proxies.proxy-protocol",
        "value": "${USES_PROXY_PROTOCOL}",
        "value-type": "bool"
      }
    },
    {
      "$set": {
        "path": "$.messages.use-display-name-in-quit-message",
        "value": true
      }
    }
  ]
}
